{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-lakehouseproj-dev"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=swo-appsvc-dwh-sql-dev.database.windows.net;Initial Catalog=WideWorldImporters"
		},
		"ls_lake_bronz_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_lake_bronz'"
		},
		"ls_source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_source'"
		},
		"ls_archive_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://archivelakehouseprojdev.dfs.core.windows.net/"
		},
		"ls_databricks_WS_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1202-084555-aaqmz7am"
		},
		"ls_lake_bronz_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://westcancercenterstorage.dfs.core.windows.net/"
		},
		"ls_lakehouse_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://lakelakehouseprojdev.dfs.core.windows.net/"
		},
		"ls_lakehouse_keyvaults_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvlt-lakehouseproj-dev.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-lakehouseproj-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"ls_lake_bronz_accountKey": {
						"value": "[parameters('ls_lake_bronz_accountKey')]"
					},
					"ls_source_connectionString": {
						"value": "[parameters('ls_source_connectionString')]"
					},
					"ls_lake_bronz_properties_typeProperties_url": {
						"value": "[parameters('ls_lake_bronz_properties_typeProperties_url')]"
					},
					"ls_lakehouse_properties_typeProperties_url": {
						"value": "[parameters('ls_lakehouse_properties_typeProperties_url')]"
					},
					"ls_lakehouse_keyvaults_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_lakehouse_keyvaults_properties_typeProperties_baseUrl')]"
					},
					"ls_archive_properties_typeProperties_url": {
						"value": "[parameters('ls_archive_properties_typeProperties_url')]"
					},
					"ls_databricks_WS_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_WS_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-lakehouseproj-dev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-lakehouseproj-dev_ArmTemplate_0')]"
			]
		}
	]
}